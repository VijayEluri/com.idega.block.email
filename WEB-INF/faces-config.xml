<?xml version="1.0" encoding="UTF-8"?>

<faces-config
	xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
version="1.2">
	<component>
		<component-type>E-mail sender</component-type>
		<component-class>com.idega.block.email.presentation.EmailSender</component-class>
		<property>
			<display-name>From</display-name>
			<property-name>from</property-name>
			<property-class>java.lang.String</property-class>
		</property>
		<property>
			<display-name>To</display-name>
			<property-name>recipientTo</property-name>
			<property-class>java.lang.String</property-class>
		</property>
		<property>
			<display-name>Cc</display-name>
			<property-name>recipientCc</property-name>
			<property-class>java.lang.String</property-class>
		</property>
		<property>
			<display-name>Bcc</display-name>
			<property-name>recipientBcc</property-name>
			<property-class>java.lang.String</property-class>
		</property>
		<property>
			<display-name>Subject</display-name>
			<property-name>subject</property-name>
			<property-class>java.lang.String</property-class>
		</property>
		<property>
			<display-name>Message</display-name>
			<property-name>message</property-name>
			<property-class>java.lang.String</property-class>
		</property>
		<component-extension>
			<idegaweb-info>
				<builder-visible>true</builder-visible>
				<object-type>jsf.uicomponent</object-type>
			</idegaweb-info>
		</component-extension>
	</component>
</faces-config>